{
  "name": "vitamin-hair-backend",
  "version": "0.1.0",
  "description": "API Project for Vitamin Hair App",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/server",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "babel src -d dist",
    "predev": "babel-node src/tools/startMessage.js",
    "dev": "npm-run-all --parallel security-check lint open:src",
    "open:src": "babel-watch src/server.js",
    "security-check": "nsp check",
    "lint": "eslint src/server.js"
  },
  "author": "quangnd",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bookshelf": "^0.10.4",
    "chalk": "^2.1.0",
    "dotenv": "^4.0.0",
    "good": "^7.3.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.6.2",
    "jsonwebtoken": "^8.0.1",
    "knex": "^0.13.0",
    "mysql": "^2.15.0",
    "nsp": "^2.8.1"
  },
  "devDependencies": {
    "babel-watch": "^2.0.7",
    "eslint": "^4.7.2",
    "eslint-watch": "^3.1.2",
    "npm-run-all": "^4.1.1"
  }
}
